{
  "log": {
    "loglevel": "warning"
  },
  "inbounds": [
    "listen": "127.0.0.1",
    "port": V2RAY_CLIENT_PORT,
    "protocol": "socks",
    "settings": {
      "auth": "noauth",
      "udp": true
    }
  }
],
"outbounds": [
  {
    "protocol": "vless",
    "settings": {
      "vnext": [
        {
          "address": "V2RAY_SERVER_ADDRESS",
          "port": 443,
          "users": [
            {
              "id": "V2RAY_CLIENT_UUID",
              "encryption": "none",
              "level": 0
            }
          ]
        }
      ]
    },
    "streamSettings": {
      "network": "tcp",
      "security": "tls",
      "tlsSettings": {
        "serverName": "V2RAY_SERVER_ADDRESS",
        "allowInsecure": false
      }
    }
  }
]
}
